
@{
    ViewBag.Title = "403";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<header class="header_in is_sticky menu_fixed">@Response.StatusCode Hatası</header>

<main>
    <div id="error_page">
        <div class="wrapper">
            <div class="container">
                <div class="row justify-content-center text-center">
                    <div class="col-xl-7 col-lg-9">
                        <h2>@Response.StatusCode <i class="icon_error-triangle_alt"></i></h2>
                        <p>Belirlenen yol bulunamadı.</p>
                    </div>
                </div>
                <!-- /row -->
            </div>
            <!-- /container -->
        </div>
        <!-- /wrapper -->
    </div>
    <!-- /error_page -->
</main>
